{
    "name": "dockview-react",
    "version": "1.0.0",
    "description": "",
    "main": "dist/cjs/index.js",
    "types": "dist/cjs/index.d.ts",
    "module": "dist/es6/index.js",
    "scripts": {
        "build-gulp": "cross-env ../../node_modules/.bin/gulp run",
        "docs": "typedoc",
        "build-css": "gulp sass",
        "build-es6": "tsc --project tsconfig.es6.json --extendedDiagnostics",
        "build-cjs": "tsc --project tsconfig.json --extendedDiagnostics",
        "build": "npm run build-cjs && npm run build-es6 && npm run build-css",
        "clean": "rimraf dist/ .build/",
        "rebuild": "npm run clean && npm run build",
        "test": "bash ../../node_modules/.bin/jest --selectProjects dockview-react",
        "test-cov": "cross-env ../../node_modules/.bin/jest --selectProjects dockview-react --coverage"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "dockview": "0.0.0-beta.0"
    },
    "devDependencies": {
        "@testing-library/react": "^10.4.2",
        "@types/react": "^16.9.52",
        "@types/react-dom": "^16.9.8",
        "cross-env": "^7.0.2",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.0.3"
    },
    "peerDependencies": {
        "react": "^16.8.0",
        "react-dom": "^16.8.0"
    }
}
