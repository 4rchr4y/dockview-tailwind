{
    "name": "dockview",
    "version": "0.0.0-beta.1",
    "description": "",
    "main": "dist/cjs/index.js",
    "types": "dist/cjs/index.d.ts",
    "module": "dist/es6/index.js",
    "scripts": {
        "build-gulp": "cross-env ../../node_modules/.bin/gulp run",
        "docs": "typedoc",
        "build-css": "gulp sass",
        "build-es6": "tsc --project tsconfig.es6.json --extendedDiagnostics",
        "build-cjs": "tsc --project tsconfig.json --extendedDiagnostics",
        "build": "npm run build-cjs && npm run build-es6 && npm run build-css",
        "clean": "rimraf dist/ .build/",
        "rebuild": "npm run clean && npm run build",
        "test": "cross-env ../../node_modules/.bin/jest --selectProjects dockview",
        "test-cov": "cross-env ../../node_modules/.bin/jest --selectProjects dockview --coverage"
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "splitview",
        "split-view",
        "gridview",
        "grid-view",
        "layout",
        "react",
        "react-component",
        "grid"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/react": "^16.9.53",
        "@types/react-dom": "^16.9.8",
        "cross-env": "^7.0.2",
        "gulp": "^4.0.2",
        "node-sass": "^4.14.1",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "rimraf": "^3.0.2",
        "typedoc": "^0.18.0",
        "typescript": "^4.0.3"
    },
    "peerDependencies": {
        "react": "^16.8.0",
        "react-dom": "^16.8.0"
    },
    "dependencies": {}
}
