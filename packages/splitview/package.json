{
    "name": "dockview",
    "version": "0.0.4",
    "description": "Zero dependency layout manager supporting tabs, grids and splitviews with ReactJS support",
    "main": "dist/cjs/index.js",
    "types": "dist/cjs/index.d.ts",
    "module": "dist/es6/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/mathuo/dockview.git"
    },
    "homepage": "https://github.com/mathuo/dockview",
    "scripts": {
        "build-gulp": "cross-env ../../node_modules/.bin/gulp run",
        "docs": "typedoc",
        "build-css": "gulp sass",
        "build-es6": "cross-env ../../node_modules/.bin/tsc --project tsconfig.es6.json --extendedDiagnostics",
        "build-cjs": "cross-env ../../node_modules/.bin/tsc --project tsconfig.json --extendedDiagnostics",
        "build": "npm run build-cjs && npm run build-es6 && npm run build-css",
        "clean": "rimraf dist/ .build/",
        "rebuild": "npm run clean && npm run build",
        "test": "cross-env ../../node_modules/.bin/jest --selectProjects dockview",
        "test-cov": "cross-env ../../node_modules/.bin/jest --selectProjects dockview --coverage",
        "try-strict": "cross-env ../../node_modules/.bin/tsc --project tsconfig.strictNullChecks.json --extendedDiagnostics",
        "preversion": "npm run rebuild && npm run test"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "keywords": [
        "splitview",
        "split-view",
        "gridview",
        "grid-view",
        "dockview",
        "dock-view",
        "tabs",
        "layout",
        "layout manager",
        "dock layout",
        "docking",
        "react",
        "react-component",
        "grid"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@testing-library/react": "^11.2.2",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "cross-env": "^7.0.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "rimraf": "^3.0.2",
        "typedoc": "^0.18.0"
    },
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    }
}
