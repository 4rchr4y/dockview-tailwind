{
    "compilerOptions": {
        "module": "es6",
        "declaration": true,
        "target": "es6",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "downlevelIteration": true,
        "incremental": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "allowUnreachableCode": false,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "lib": [
			"ES2015",
			"ES2016.Array.Include",
			"ES2017.String",
			"ES2018.Promise",
			"DOM",
		]
        
    }
}